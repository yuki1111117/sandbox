{
  "rules": {
    "scene": {
      "$uid": {
        ".read": true,
        ".write": "$uid === auth.uid"
      }
    },
    "chat": {
      ".read": true,
      ".write": true,
      "$uid": {
        ".read": true,
        ".write": "$uid === auth.uid"
      }
    },
    "memo": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "cookie": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "private": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "publicR": {
      "$uid": {
        ".read": true,
        ".write": "$uid === auth.uid"
      }
    },
    "publicRW": {
      "$uid": {
        ".read": true,
        ".write": true
      }
    }
  }
}
